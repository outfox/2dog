<Project Sdk="Microsoft.NET.Sdk">
  <!-- Platform-specific configuration -->
  <PropertyGroup>
    <PlatformRID>osx-x64</PlatformRID>
    <PlatformOS>macos</PlatformOS>
    <BuildVariant>editor</BuildVariant>
    <NativeLibExt>dylib</NativeLibExt>
    <PackageSuffix>.editor</PackageSuffix>
    <Description>macOS x64 editor native libgodot library for twodog. Reference this package alongside twodog for macOS editor support.</Description>
  </PropertyGroup>
</Project>

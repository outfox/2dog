<Project Sdk="Microsoft.NET.Sdk">
  <!-- Platform-specific configuration -->
  <PropertyGroup>
    <PlatformRID>osx-x64</PlatformRID>
    <PlatformOS>macos</PlatformOS>
    <BuildVariant>template_debug</BuildVariant>
    <NativeLibExt>dylib</NativeLibExt>
    <PackageSuffix>.debug</PackageSuffix>
    <Description>macOS x64 debug native libgodot library for twodog. Reference this package alongside twodog for macOS debug support.</Description>
  </PropertyGroup>
</Project>

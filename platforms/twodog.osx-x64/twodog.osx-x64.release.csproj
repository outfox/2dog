<Project Sdk="Microsoft.NET.Sdk">
  <!-- Platform-specific configuration -->
  <PropertyGroup>
    <PlatformRID>osx-x64</PlatformRID>
    <PlatformOS>macos</PlatformOS>
    <BuildVariant>template_release</BuildVariant>
    <NativeLibExt>dylib</NativeLibExt>
    <PackageSuffix>.release</PackageSuffix>
    <Description>macOS x64 release native libgodot library for twodog. Reference this package alongside twodog for macOS release support.</Description>
  </PropertyGroup>
</Project>

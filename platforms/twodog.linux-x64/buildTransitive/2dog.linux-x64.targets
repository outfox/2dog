<Project>
    <!--
      TwoDog Linux x64 Meta Package Transitive Targets

      This file is imported by projects that transitively reference 2dog.linux-x64.
      It imports the main targets from the build folder.
    -->
    <Import Project="$(MSBuildThisFileDirectory)../build/2dog.linux-x64.targets"
            Condition="Exists('$(MSBuildThisFileDirectory)../build/2dog.linux-x64.targets')"/>
</Project>

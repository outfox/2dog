<Project Sdk="Microsoft.NET.Sdk">
  <!-- Platform-specific configuration -->
  <PropertyGroup>
    <PlatformRID>win-x64</PlatformRID>
    <PlatformOS>windows</PlatformOS>
    <BuildVariant>template_release</BuildVariant>
    <NativeLibExt>dll</NativeLibExt>
    <PackageSuffix>.release</PackageSuffix>
    <Description>Windows x64 release native libgodot library for twodog. Reference this package alongside twodog for Windows release support.</Description>
  </PropertyGroup>
</Project>
